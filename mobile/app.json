{
  "expo": {
    "name": "Heka",
    "slug": "heka",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "heka",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.heka.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.heka.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#0F172A",
      "barStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {}
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6ed8b76f-a657-4a77-8ede-c2a0fcffc794"
      }
    },
    "owner": "mo.zeyada"
  }
}
